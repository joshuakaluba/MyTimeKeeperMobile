{
  "expo": {
    "name": "My Time Keeper",
    "slug": "MyTimeKeeperMobile",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3498db"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.joshuakaluba.timekeeper"
    },
    "android": {
      "package": "com.joshuakaluba.timekeeper",
      "permissions": ["VIBRATE", "NOTIFICATIONS"],
      "versionCode": 3
    }
  }
}
